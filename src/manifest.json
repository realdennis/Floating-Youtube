{
  "name": "Floating Youtube",
  "description": "[Youtube] Auto float video to corner when it's not in user's view.",
  "version": "2.5",
  "background": {},
  "browser_action": {},
  "manifest_version": 2,
  "content_scripts": [
    {
      "matches": ["*://www.youtube.com/*"],
      "js": ["./index.js"]
    }
  ],
  "icons": {
    "16": "icon.png",
    "48": "icon.png",
    "128": "icon.png"
  }
}
